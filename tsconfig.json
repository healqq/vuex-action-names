{
  "compilerOptions": {
      "sourceMap": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strictFunctionTypes": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
  },
  "include": [
      "src/**/*",
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}
